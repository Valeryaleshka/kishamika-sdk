{
  "name": "kikamisha-sdk",
  "version": "0.1.4",
  "description": "kishamika SDK shared",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Valeryaleshka/kishamika-sdk.git"
  },
  "license": "MIT",
  "author": "Valery Aleshka <valeryaleshka@gmail.com>",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./helpers": {
      "import": "./dist/helpers/index.js",
      "types": "./dist/helpers/index.d.ts"
    },
    "./styles": "./dist/styles/index.less"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:ts && npm run build:styles",
    "build:ts": "tsc",
    "build:styles": "copyfiles -u 1 \"src/styles/**/*\" dist/",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "copyfiles": "^2.4.1"
  },
  "dependencies": {
    "prompts": "^2.4.2"
  }
}
